        section.benefits
          .container.benefits__container
            +sectionTitle('Benefits of GrowDirector Pro','benefits__title', 'fade-up', '500', 'ease-in-out', '0')
            ul.benefits__list.row
              -
                const values = [
                  'Easy to use', 
                  'Access from anywhere', 
                  'Set it fast', '3-year warranty ', 
                  'Open API', 'Scalability'
                ]
              each val, idx in values
                li.benefits__item.col-md-4.col-sm-6(data-aos="fade-up" data-aos-duration="500" data-aos-easing="ease-in-out" data-aos-delay="200")
                  svg.benefits__icon
                    use(xlink:href="@img/sprite.svg#home-benefit-" + (+idx + 1))
                  h3.benefits__heading= val
                  a.benefits__link(href="#")
                    span.benefits__link-text More information
                    svg.benefits__link-icon
                      use(xlink:href="@img/sprite.svg#arrow-long")
        
